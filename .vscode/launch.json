    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch example-ng6-tester",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/projects/example-ng6-tester",
            "sourceMapPathOverrides": {
                "webpack:///ng://example-ng6-lib/lib/*": "${workspaceFolder}/projects/example-ng6-lib/src/lib/*",
                "webpack:///./src/*": "${workspaceFolder}/projects/example-ng6-tester/src/*"
            }
        }
    ]
}
